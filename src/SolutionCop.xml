<?xml version="1.0" encoding="utf-8"?>
<Rules>
  <TargetFrameworkVersion enabled="true">
    <AllowedValue>4.0</AllowedValue>
  </TargetFrameworkVersion>
  <ReferenceNuGetPackagesOnly enabled="true" />
  <TreatWarningsAsErrors enabled="true">
    <AllWarnings />
  </TreatWarningsAsErrors>
  <SuppressWarnings enabled="false"></SuppressWarnings>
  <WarningLevel>
    <MinimalValue>4</MinimalValue>
  </WarningLevel>
  <AllowOnlySpecificNuGetPackages enabled="false" />
  <NuGetPackageReferencedInProject enabled="false">
    <Exceptions>
      <Exception>Rx-Main</Exception>
    </Exceptions>
  </NuGetPackageReferencedInProject>
  <StyleCopEnabled enabled="false" />
  <VerifyNuGetPackageVersionsRule enabled="false" />
  <NuGetAutomaticPackagesRestore enabled="false">
    <Exception>FakeProject.csproj</Exception>
  </NuGetAutomaticPackagesRestore>
  <NuGetPackagesUsage enabled="false">
    <Exception>Rx-Main</Exception>
  </NuGetPackagesUsage>
  <NuGetPackageVersions enabled="false" />
</Rules>