<?xml version="1.0" encoding="utf-8"?>
<Rules>
  <TargetFrameworkVersion enabled="true">
    <FrameworkVersion>4.0</FrameworkVersion>
  </TargetFrameworkVersion>
  <ReferenceNuGetPackagesOnly enabled="true" />
  <TreatWarningsAsErrors enabled="true">
    <AllWarnings />
  </TreatWarningsAsErrors>
  <SuppressWarnings enabled="true"></SuppressWarnings>
  <WarningLevel>
    <MinimalValue>4</MinimalValue>
  </WarningLevel>
  <StyleCopEnabled enabled="false" />
  <NuGetAutomaticPackagesRestore enabled="true"></NuGetAutomaticPackagesRestore>
  <NuGetPackagesUsage enabled="true"></NuGetPackagesUsage>
  <NuGetPackageVersions enabled="false" />
  <TreatStyleCopWarningsAsErrors enabled="false"></TreatStyleCopWarningsAsErrors>
  <SameNuGetPackageVersions enabled="true"></SameNuGetPackageVersions>
  <FilesIncludedIntoProject enabled="true">
    <FileName>*.cs</FileName>
    <Exception>
      <Project>ProjectToExcludeFromCheck.csproj</Project>
    </Exception>
    <Exception>
      <FileName>My.File.cs</FileName>
    </Exception>
  </FilesIncludedIntoProject>
</Rules>